<includes>
		<include name="ListView">
				<control type="group">
						<visible>Control.IsVisible(58) + !Control.IsVisible(50) + !Window.IsVisible(134) + !Window.IsVisible(3000)</visible>
						<control type="group">
								<control type="image">
										<left>100</left>
										<top>588</top>
										<width>1080</width>
										<height>30</height>
										<aspectratio>stretch</aspectratio>
										<colordiffuse>FF000000</colordiffuse>
										<texture>views/list/listview_shadow.png</texture>
								</control>
								<control type="image">
										<left>120</left>
										<top>150</top>
										<width>320</width>
										<height>450</height>
										<aspectratio>stretch</aspectratio>
										<texture background="true">no_poster.jpg</texture>
										<bordertexture border="5">poster-border.png</bordertexture>
										<bordersize>5</bordersize>
								</control>
								<control type="image">
										<left>120</left>
										<top>150</top>
										<width>320</width>
										<height>450</height>
										<aspectratio>keep</aspectratio>
										<fadetime>ThumbCrossfadeTime</fadetime>
										<texture background="true">$INFO[listitem.icon]</texture>
										<bordertexture>-</bordertexture>
										<bordersize>5</bordersize>
										<visible>!Skin.HasSetting(_resourcesenabled)</visible>
								</control>
								<control type="group">
										<visible>Skin.HasSetting(_resourcesenabled)</visible>
										<control type="image">
												<left>120</left>
												<top>150</top>
												<width>320</width>
												<height>450</height>
												<aspectratio>keep</aspectratio>
												<fadetime>ThumbCrossfadeTime</fadetime>
												<texture background="true">$INFO[listitem.icon]</texture>
												<bordertexture>-</bordertexture>
												<bordersize>5</bordersize>
												<visible>ListItem.IsFolder</visible>
										</control>
										<control type="largeimage">
												<left>120</left>
												<top>150</top>
												<width>320</width>
												<height>450</height>
												<aspectratio>stretch</aspectratio>
												<fadetime>ThumbCrossfadeTime</fadetime>
												<texture background="true">$INFO[listitem.path,,_resources\artwork\poster.jpg]</texture>
												<bordertexture>-</bordertexture>
												<bordersize>5</bordersize>
										</control>
								</control>
								<control type="list" id="58">
										<left>440</left>
										<top>150</top>
										<width>740</width>
										<height>450</height>
										<onleft>9000</onleft>
										<onright>90</onright>
										<onup>58</onup>
										<ondown>58</ondown>
										<viewtype label="view58">List</viewtype>
										<pagecontrol>90</pagecontrol>
										<scrolltime>230</scrolltime>
										<preloaditems>4</preloaditems>
										<itemlayout height="90" width="720">
												<control type="image">
														<left>0</left>
														<top>0</top>
														<width>720</width>
														<height>90</height>
														<texture background="true" border="5">views/list/listnf.png</texture>
												</control>
												<control type="image">
														<left>10</left>
														<top>10</top>
														<width>70</width>
														<height>70</height>
														<texture background="true">no_cd_small.png</texture>
												</control>
												<control type="image">
														<left>10</left>
														<top>10</top>
														<width>70</width>
														<height>70</height>
														<texture background="true" diffuse="views/list/CD_diffuse_listview.png">$INFO[listitem.path,,_resources\artwork\cd_small.jpg]</texture>
														<visible>Skin.HasSetting(_resourcesenabled)</visible>
												</control>
												<control type="label">
														<left>90</left>
														<top>10</top>
														<width>620</width>
														<height>45</height>
														<font>size_35</font>
														<textcolor>view58_info_label</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$INFO[ListItem.Label]</label>
												</control>
												<control type="label">
														<left>90</left>
														<top>55</top>
														<width>620</width>
														<height>20</height>
														<font>size_15</font>
														<textcolor>view58_info_label</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$LOCALIZE[31187] $LOCALIZE[31191]</label>
														<visible>!StringCompare(ListItem.Label,Music) + !StringCompare(ListItem.Label,..) + StringCompare(ListItem.ProgramCount,0)</visible>
												</control>
												<control type="label">
														<left>90</left>
														<top>55</top>
														<width>620</width>
														<height>20</height>
														<font>size_15</font>
														<textcolor>view58_info_label</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$LOCALIZE[31187] $INFO[ListItem.ProgramCount,, $LOCALIZE[31188]]</label>
														<visible>!StringCompare(ListItem.Label,Music) + !StringCompare(ListItem.Label,..) + StringCompare(ListItem.ProgramCount,1)</visible>
												</control>
												<control type="label">
														<left>90</left>
														<top>55</top>
														<width>620</width>
														<height>20</height>
														<font>size_15</font>
														<textcolor>view58_info_label</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$LOCALIZE[31187] $INFO[ListItem.ProgramCount,, $LOCALIZE[31189]]</label>
														<visible>!StringCompare(ListItem.Label,Music) + !StringCompare(ListItem.Label,..) + IntegerGreaterThan(ListItem.ProgramCount,1)</visible>
												</control>
												<control type="image">
														<left>680</left>
														<top>50</top>
														<width>30</width>
														<height>30</height>
														<aspectratio>stretch</aspectratio>
														<texture>$INFO[listitem.overlay]</texture>
												</control>
										</itemlayout>
										<focusedlayout height="90" width="720">
												<control type="image">
														<left>0</left>
														<top>0</top>
														<width>720</width>
														<height>90</height>
														<texture background="true" border="5">views/list/thumbfo.png</texture>
												</control>
												<control type="image">
														<left>10</left>
														<top>10</top>
														<width>70</width>
														<height>70</height>
														<texture background="true">no_cd_small.png</texture>
												</control>
												<control type="image">
														<left>10</left>
														<top>10</top>
														<width>70</width>
														<height>70</height>
														<texture background="true" diffuse="views/list/CD_diffuse_listview.png">$INFO[listitem.path,,_resources\artwork\cd_small.jpg]</texture>
														<visible>Skin.HasSetting(_resourcesenabled)</visible>
												</control>
												<control type="image">
														<left>680</left>
														<top>50</top>
														<width>30</width>
														<height>30</height>
														<aspectratio>stretch</aspectratio>
														<texture>$INFO[listitem.overlay]</texture>
												</control>
												<control type="label">
														<left>90</left>
														<top>10</top>
														<width>620</width>
														<height>45</height>
														<font>size_35</font>
														<textcolor>view58_info_label_alt</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$INFO[ListItem.Label]</label>
												</control>
												<control type="label">
														<left>90</left>
														<top>55</top>
														<width>620</width>
														<height>20</height>
														<font>size_15</font>
														<textcolor>view58_info_label_alt</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$LOCALIZE[31187] $LOCALIZE[31191]</label>
														<visible>!StringCompare(ListItem.Label,Music) + !StringCompare(ListItem.Label,..) + StringCompare(ListItem.ProgramCount,0)</visible>
												</control>
												<control type="label">
														<left>90</left>
														<top>55</top>
														<width>620</width>
														<height>20</height>
														<font>size_15</font>
														<textcolor>view58_info_label_alt</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$LOCALIZE[31187] $INFO[ListItem.ProgramCount,, $LOCALIZE[31188]]</label>
														<visible>!StringCompare(ListItem.Label,Music) + !StringCompare(ListItem.Label,..) + StringCompare(ListItem.ProgramCount,1)</visible>
												</control>
												<control type="label">
														<left>90</left>
														<top>55</top>
														<width>620</width>
														<height>20</height>
														<font>size_15</font>
														<textcolor>view58_info_label_alt</textcolor>
														<align>left</align>
														<aligny>center</aligny>
														<label>$LOCALIZE[31187] $INFO[ListItem.ProgramCount,, $LOCALIZE[31189]]</label>
														<visible>!StringCompare(ListItem.Label,Music) + !StringCompare(ListItem.Label,..) + IntegerGreaterThan(ListItem.ProgramCount,1)</visible>
												</control>
										</focusedlayout>
								</control>
						</control>
						<control type="scrollbar" id="90">
								<left>1160</left>
								<top>150</top>
								<width>8</width>
								<height>450</height>
								<onleft>58</onleft>
								<include condition="Control.IsVisible(58)">ViewScrollBarVer</include>
						</control>
				</control>
		</include>
</includes>